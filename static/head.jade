head
	title=titleString
	
	//css
	link(href="/static/style.css" type="text/css" rel="stylesheet")
	
	//include jquery
	script(src="/static/js/jquery-3.1.0.min.js")
	script(src="/static/js/jquery.tablesorter.min.js")
	
	//-include nicer text area
	script(src="//cdn.tinymce.com/4/tinymce.min.js")
	script.
		tinymce.init({
		  selector: 'textarea',
		  height: 300,
		  plugins: [
			'advlist autolink lists link image charmap print preview anchor',
			'searchreplace visualblocks code fullscreen',
			'insertdatetime media table contextmenu paste code'
		  ],
		  toolbar: 'insertfile undo redo | styleselect | fontsizeselect | bold italic underline | alignleft aligncenter alignright alignjustify | bullist numlist outdent indent | link image',
		  content_css: [
			'//www.tinymce.com/css/codepen.min.css'
		  ],
		  fontsize_formats: '8pt 10pt 12pt 14pt 18pt 24pt 36pt',
		  
		  init_instance_callback: "load_form_contents"
		});
	
	//- conditional browser thing
	<!--[if lt IE 9]>
	<script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]]-->